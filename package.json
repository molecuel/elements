    "@types/gulp-plumber": "^0.0.30",
    "@types/gulp-tslint": "^3.6.30",
    "@types/gulp-typescript": "^0.0.33",
    "@types/lodash": "^4.14.42",
    "@types/merge2": "^0.3.30",
    "@types/mocha": "^2.2.33",
    "@types/should": "^8.1.30",
    "@types/when": "^2.4.28",
    "assert": ">=0.0.1",
    "coveralls": "^2.11.15",
    "del": "^2.2.0",
    "gulp": "^3.9.1",
    "gulp-gh-pages": "^0.5.4",
    "gulp-plumber": "^1.1.0",
    "gulp-sourcemaps": "^2.2.2",
    "gulp-tsconfig-files": "^0.2.5",
    "gulp-tslint": "^7.0.1",
    "gulp-typedoc": "^2.0.1",
    "gulp-typescript": "^3.1.3",
    "gulp-util": "^3.0.7",
    "gulpclass": "0.1.2",
    "istanbul": "^0.4.5",
    "merge2": "^1.0.1",
    "mocha": "^3.2.0",
    "mocha-lcov-reporter": "^1.2.0",
    "remap-istanbul": "0.9.1",
    "should": ">= 0.0.1",
    "tslint": "^4.4.2",
    "tslint-stylish": "^2.1.0-beta",
    "typedoc": "^0.5.1",
    "typescript": "^2.2.1"
  },
  "dependencies": {
    "@molecuel/core": "^2.0.11",
